.index {
  display: flex;
  align-items: center;
  flex-direction: column;
  padding: 50rpx;
}
.list {
  display: flex;
}
.list .list-item {
  width: 200rpx;
  height: 200rpx;
  border-radius: 15rpx;
  border: 1rpx solid #ebebeb;
  display: flex;
  justify-content: center;
  align-items: center;
}